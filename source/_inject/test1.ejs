<div class="tutorials-container">
  {% for post in site.posts %}
    {% if post.categories contains 'tutorials' %}
      <div class="tutorial-item">
        <a href="{{ post.url }}">
          <img src="{{ post.cover }}" alt="{{ post.title }}">
          <h3>{{ post.title }}</h3>
        </a>
      </div>
    {% endif %}
  {% endfor %}
</div>
